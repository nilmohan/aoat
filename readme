export const startEditUser = (id, updates) => {
    return (dispatch, getState) => {

        return storage.child(`profile/${id}`).put(image[0]).then((snapshot) => {
                let updatedUser = {...updates, pictureUrl: snapshot.metadata.downloadURLs[0]};
        return database.ref(`users/${id}`).update(updatedUser).then(() => {
                dispatch(editUser(id, updatedUser));
        });
    };
};